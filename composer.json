{
    "name": "ipapikas/alerter",
    "description": "Alerter creates a notification schema that allows you to send event-based notifications to subscribers.",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/ioannis-papikas/alerter/issues",
        "source": "https://github.com/ioannis-papikas/alerter"
    },
    "authors": [
        {
            "name": "Ioannis Papikas",
            "email": "papikas.ioan@gmail.com",
            "homepage": "https://ipapikas.me"
        }
    ],
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5"
    },
    "autoload": {
        "psr-4": {
            "Alerter\\": "src/Alerter"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alerter\\": "tests/Alerter"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}